<SCRIPT LANGUAGE="JScript">
	var MyTour = newTour();
	
	function Initialize()
	{
		PositionContainers();
		
		MyTour.newScene("", 1, "ClickTopicButton(1);", 31000, "", "wmpaud1.wav");
		MyTour.newScene("", 2, "ClickTopicButton(2);", 20000, "Viz.wmv", "wmpaud2.wav");
		MyTour.newScene("", 3, "ClickTopicButton(3);", 16000, "", "wmpaud3.wav");
		MyTour.newScene("", 4, "ClickTopicButton(4);", 31000, "CopyCD.wmv", "wmpaud4.wav");
		MyTour.newScene("", 5, "ClickTopicButton(5);", 26000, "MdLib.wmv", "wmpaud5.wav");
		MyTour.newScene("", 6, "ClickTopicButton(6);", 24000, "", "wmpaud6.wav");
		MyTour.newScene("", 7, "ClickTopicButton(7);", 16000, "RTuner.wmv", "wmpaud7.wav");
		MyTour.newScene("", 8, "ClickTopicButton(8);", 26000, "NuSkin.wmv", "wmpaud8.wav");
		MyTour.newScene("", 9, "ClickTopicButton(9);", 26000, "", "wmpaud9.wav");
		
		document.title = getText("txtApplicationTitle");
		setTitles();
		
		WriteTopicButtonsCaption();
		TopicButtonEVENTonclick(1);
		idLNKWindowsMedia = getText("titleTimeElapsed");
		idTourContainer.style.display = "";
		idTimeElapsed.title = getText("titleTimeElapsed");
		idLNKWindowsMedia.href = idContents.L_URLHREF2; //"http://www.microsoft.com/"
		MyTour.start();
		
		idBody.focus();
	
	}
</SCRIPT>
</HEAD>
