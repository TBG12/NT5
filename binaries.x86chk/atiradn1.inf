DriverVer=06/06/2001,5.01.2001

[DestinationDirs]
DefaultDestDir      = 11
atimtag.Miniport    = 12  ; drivers
atimtag.Display     = 11  ; system32
atimtag.Display_M6  = 11  ; system32

[ControlFlags]
ExcludeFromSelect=*

;
; Driver information
;

[Manufacturer]
%ATI% = ATI.Mfg

[ATI.Mfg]

;%RADEON_ME% (ATI)  = atimtag_M6, PCI\VEN_1002&DEV_4C59&SUBSYS_4C591002
