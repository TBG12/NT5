[atimtag_M6]
; Include=msdv.inf
CopyFiles=atimtag.Miniport, atimtag.Display_M6, atimtag.OpenGL

DelReg=atimtag_SoftwareDesktopSettings, atimtag_SoftwareDesktopSettings_ENU
