<SCWKBRegistration>

<!-- Conflict Resolution Algorithm:
//
// 1. System (for services only)
// 2. Custom (last one at this level wins)
// 3. Root
// 4. Extensions (last one at this level wins)
//
-->

	<KBs OSVersionMajorInfo = "5" OSVersionMinorInfo = "2" OSDescription = "Windows Server 2003">
		<Root>
			<Name> W2k3.xml </Name>
		</Root>
		<Custom>
		</Custom>
		<Extensions>
			<Name> CommerceServer.xml </Name>
			<Name> ISA.xml </Name>
			<Name> Exchange.xml </Name>
			<Name> OperationsManager2000.xml </Name>
			<Name> SQLServer2000.xml </Name>
			<Name> STS.xml </Name>
			<Name> SPS.xml </Name>
			<Name> SMS.xml </Name>
		</Extensions>
	</KBs>

