<SimpleDialog>
	<Title>
		<Text><![CDATA[Did you find what you wanted?]]></Text>
        <Property Name="TitleTemplate">There were %1 files found. Did you find what you wanted?</Property>
        <Property Name="OneTitleTemplate">There was one file found.  Is it what you wanted?</Property>
        <Property Name="EmptyTitleTemplate">There were no files found.  Do you want to quit searching?</Property>
	</Title>
	<Link Id="5014" IconId="290" TopMargin="8">
		<Text><![CDATA[Yes, finished searching]]></Text>
	</Link>
	<Link Id="5772" IconId="290" TopMargin="8">
		<Text><![CDATA[Yes, but make future searches faster]]></Text>
        <Property Name="RefineCondition">FewResults AND ShowIndexServer</Property>
	</Link>
	<Title TopMargin="8">
		<Text><![CDATA[No, refine this search and...]]></Text>
	</Title>
	<Link Id="5752" IconId="290" TopMargin="8">
		<Text><![CDATA[Specify file name or keywords]]></Text>
        <Property Name="RefineCondition">ManyResults AND NOT FileNameSearch AND NOT KeywordsSearch</Property>
	</Link>
	<Link Id="5753" IconId="290" TopMargin="5">
		<Text><![CDATA[Change file name or keywords]]></Text>
        <Property Name="RefineCondition">FileNameSearch OR KeywordsSearch</Property>
	</Link>
	<Link Id="5754" IconId="290" TopMargin="5">
		<Text><![CDATA[Search for file name instead of keywords]]></Text>
        <Property Name="RefineCondition">ManyResults AND KeywordsSearch AND NOT FileNameSearch</Property>
	</Link>
	<Link Id="5756" IconId="290" TopMargin="5">
		<Text><![CDATA[Look in a specific location]]></Text>
        <Property Name="RefineCondition">ManyResults AND NOT LookInSearch</Property>
	</Link>
	<Link Id="5757" IconId="290" TopMargin="5">
		<Text><![CDATA[Look in more locations]]></Text>
        <Property Name="RefineCondition">FewResults AND LookInSearch</Property>
	</Link>
	<Link Id="5758" IconId="290" TopMargin="5">
		<Text><![CDATA[Include hidden and system files]]></Text>
        <Property Name="RefineCondition">NOT SystemSearch AND NOT HiddenSearch</Property>
	</Link>
	<Link Id="5759" IconId="290" TopMargin="5">
		<Text><![CDATA[Don't include hidden and system files]]></Text>
        <Property Name="RefineCondition">ManyResults AND SystemSearch AND HiddenSearch</Property>
	</Link>
	<Link Id="5760" IconId="290" TopMargin="5">
		<Text><![CDATA[Change whether hidden and system files are included]]></Text>
        <Property Name="RefineCondition">SystemSearch AND NOT HiddenSearch</Property>
	</Link>
	<Link Id="5761" IconId="290" TopMargin="5">
		<Text><![CDATA[Change whether hidden and system files are searched]]></Text>
        <Property Name="RefineCondition">NOT SystemSearch AND HiddenSearch</Property>
	</Link>
	<Link Id="5762" IconId="290" TopMargin="5">
		<Text><![CDATA[Find only pictures larger than 2 KB]]></Text>
        <Property Name="RefineCondition">ManyResults AND PictureSearch AND NOT SizeSearch</Property>
	</Link>
	<Link Id="5765" IconId="290" TopMargin="5">
		<Text><![CDATA[Search for all files of this type]]></Text>
        <Property Name="RefineCondition">FileNameSearch OR KeywordsSearch OR SizeSearch OR DateSearch OR LookInSearch AND FewResults AND MediaSearch</Property>
	</Link>
	<Link Id="5766" IconId="290" TopMargin="5">
		<Text><![CDATA[Specify a different file size]]></Text>
        <Property Name="RefineCondition">SizeSearch</Property>
	</Link>
	<Link Id="5767" IconId="290" TopMargin="5">
		<Text><![CDATA[Change the file dates you are looking for]]></Text>
        <Property Name="RefineCondition">DateSearch</Property>
	</Link>
	<Link Id="5768" IconId="290" TopMargin="5">
		<Text><![CDATA[Search for pictures on the Internet]]></Text>
        <Property Name="RefineCondition">InetActive AND PictureSearch</Property>
        <Property Name="TaskId" DataType="23">1051</Property>
        <Property Name="QueryTemplate"><![CDATA[%1 %2 pictures or photos]]></Property>
	</Link>
	<Link Id="5769" IconId="290" TopMargin="5">
		<Text><![CDATA[Search for music on the Internet]]></Text>
        <Property Name="RefineCondition">InetActive AND SoundSearch</Property>
        <Property Name="TaskId" DataType="23">546</Property>
        <Property Name="QueryTemplate"><![CDATA[%1 %2 music or sound]]></Property>
	</Link>
	<Link Id="5770" IconId="290" TopMargin="5">
		<Text><![CDATA[Search for video on the Internet]]></Text>
        <Property Name="RefineCondition">InetActive AND MovieSearch</Property>
        <Property Name="TaskId" DataType="23">84</Property>
        <Property Name="QueryTemplate"><![CDATA[%1 %2 video]]></Property>
	</Link>
	<Link Id="5771" IconId="290" TopMargin="5">
		<Text><![CDATA[Search for digital media on the Internet]]></Text>
        <Property Name="RefineCondition">InetActive AND MediaSearch AND NOT PictureSearch AND NOT SoundSearch AND NOT MovieSearch</Property>
        <Property Name="TaskId" DataType="23">1051</Property>
        <Property Name="QueryTemplate"><![CDATA[%1 %2 digital media]]></Property>
	</Link>

	<Title TopMargin="8">
		<Text><![CDATA[You may also want to...]]></Text>
        <Property Name="RefineCondition">ManyResults</Property>
	</Title>
	<Expando Id="5750" TopMargin="8">
		<Text><![CDATA[Sort results by category]]></Text>
        <Property Name="RefineCondition">ManyResults</Property>
	</Expando>
    <ExpandoGroup Id="5750">
	    <Radio Id="5730" Height="13" TopMargin="3" Group="1">
		    <Text><![CDATA[Name]]></Text>
	    </Radio>
	    <Radio Id="5731" Height="13" TopMargin="3">
		    <Text><![CDATA[Date Modified]]></Text>
	    </Radio>
	    <Radio Id="5732" Height="13" TopMargin="3">
		    <Text><![CDATA[Size]]></Text>
	    </Radio>
	    <Radio Id="5733" Height="13" TopMargin="3">
		    <Text><![CDATA[File Type]]></Text>
	    </Radio>
    </ExpandoGroup>
	<Expando Id="5751" TopMargin="3" Group="1">
		<Text><![CDATA[View results differently]]></Text>
        <Property Name="RefineCondition">ManyResults</Property>
	</Expando>
    <ExpandoGroup Id="5751">
	    <Radio Id="5720" Height="13" TopMargin="3" Group="1">
		    <Text><![CDATA[Details]]></Text>
	    </Radio>
	    <Radio Id="5721" Height="13" TopMargin="3">
		    <Text><![CDATA[Thumbnails]]></Text>
	    </Radio>
	    <Radio Id="5722" Height="13" TopMargin="3">
		    <Text><![CDATA[Tiles]]></Text>
	    </Radio>
    </ExpandoGroup>

	<Link Id="5006" IconId="23" TopMargin="10" Group="1">
		<Text><![CDATA[Start a &new search]]></Text>
	</Link>

</SimpleDialog>
