*% The invalid combination constraints do not produce the desired result in this cases,
*% Collation is turned off even though the user has it checked in the print dialog.
*%InvalidCombination: LIST(Collate.ON, PrinterHardDisk.FALSE, Memory.4MB)
*%InvalidCombination: LIST(Collate.ON, PrinterHardDisk.FALSE, Memory.8MB)
*%InvalidCombination: LIST(Collate.ON, PrinterHardDisk.FALSE, Memory.12MB)

*Feature: Collate
{
    *rcNameID: 392
    *DefaultOption: OFF
    *Option: ON
    {
        *rcNameID: =ON_DISPLAY
        *switch: PrinterHardDisk
        {
            *case: FALSE
            {
                *switch:  Memory  
                {
                    *case: 4MB
                    {
                        *Command: CmdSelect
                        {
                            *Order: JOB_SETUP.5
                            *Cmd: ""
                        }
                    }
                    *case: 8MB
                    {
                        *Command: CmdSelect
                        {
                            *Order: JOB_SETUP.5
                            *Cmd: ""
                        }
                    }
                    *case: 12MB
                    {
                        *Command: CmdSelect
                        {
                              *Order: JOB_SETUP.5
                              *Cmd: ""
                        }
                    }
                    *default
                    {
                        *Command: CmdSelect
                        {
                              *Order: JOB_SETUP.5
                              *Cmd: "@PJL SET QTY=" %d{NumOfCopies}"<0A>"
                        }
                    }
                }
            }
            *case: TRUE
            {
                *Command: CmdSelect
                {
                    *Order: JOB_SETUP.5
                    *Cmd: "@PJL SET QTY=" %d{NumOfCopies}"<0A>"
                }
            }
        }
    }

    *Option: OFF
    {
        *rcNameID: =OFF_DISPLAY
        *Command: CmdSelect
        {
            *Order: JOB_SETUP.5
            *Cmd: ""
        }
    }
}

