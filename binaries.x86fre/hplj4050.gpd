                        *Command: CmdCopies
                        {
                              *Order: DOC_SETUP.8
                              *Cmd: "<1B>&l" %d[1,999]{NumOfCopies}"X"
                        }
                    }
                    *default
                    {
                        *Command: CmdCopies
                        {
                              *Order: DOC_SETUP.8
                              *Cmd: "<1B>&l1X"
                        }
                    }
                }
            }
            *case: TRUE
            {
