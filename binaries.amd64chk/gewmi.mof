/******************************************************************************
 *
 * Class: GENdis_VPDAction
 *
 * Derived from: GENdis
 *
 * Description: A read returns a dummy array of single octets storing 00h 
 *              corresponding to the number of VPD keys. With a write various
 *              commands may be passed in the following format:
 *              Offset        Value        Description
 *              00:           00           Do Nothing.
 *                            01           Create new key/value entry.
 *                            02           Delete entry.
 *              01:           XXYY         2 octet VPD Key to delete or create
 *              03:           XXYY…        First octet stores length of VPD 
 *                                         value. Second to n-th octet stores
 *                                         the VPD value. This entry is only 
 *                                         provided with a create (01) command.
 *
 * Notes:
 *
 * Context: 
 *
 * Returns: GENdisVPDAction - array of 1 byte integers values.
 *
 ******************************************************************************/
                             
[WMI,
 dynamic: ToInstance,
 provider("WMIProv"),
 guid("{FB22787F-C009-4dd1-906D-8507BA89369D}"),
 locale("MS\\0x409"),
 WmiExpense(1),
 Description("NDIS VPD RAM")]

class GENdis_VPDAction : GENdis{
	[read] boolean Active;
	[key, read] string InstanceName;
        [read,
	 Description("Number of array elements."),
	 WmiDataId(1)] uint32 NumberElements;
	[read, 
	 write, 
	 Description("A read returns a dummy array of single octets storing " \
                     "00h coresponding to the number of VPD keys. With a "    \
                     "write various commands may be passed format specified " \
                     "in the documentation."),
	 WmiDataId(2),
         WmiSizeIs("NumberElements")] uint8 GENdisVPDAction[];
};


