*% Printer only supports collation if HardDisk is installed
*Feature: Collate
{
    *Name: "Collate"

    *DefaultOption: OFF
    *Option: ON
    {
        *rcNameID: =ON_DISPLAY
        *switch: HardDisk
        {
            *case: NotInstalled
            {
                *Command: CmdSelect
                {
                    *Order: JOB_SETUP.2
                    *Cmd: "@PJL SET QTY=1<0A>"
                }
            }
            *case: Installed
            {
                *Command: CmdSelect
                {
                    *Order: JOB_SETUP.2
                    *Cmd: "@PJL SET QTY=" %d{NumOfCopies}"<0A>"
                }
            }
        }
    }
    *Option: OFF
    {
        *rcNameID: =OFF_DISPLAY
        *Command: CmdSelect
        {
            *Order: JOB_SETUP.2
            *Cmd: "@PJL SET QTY=1<0A>"
        }
    }
}

*switch: Collate
{
    *case: OFF
    {
        *Command: CmdCopies
        {
            *Order: DOC_SETUP.13
            *Cmd: "<1B>&l" %d[1,999]{NumOfCopies}"X"
        }
    }
    *case: ON
    {
		*switch: HardDisk
		{
			*case: NotInstalled
			{
				*Command: CmdCopies
		        {
		            *Order: DOC_SETUP.13
		            *Cmd: "<1B>&l" %d[1,999]{NumOfCopies}"X"
		        }
			}
			*case: Installed
			{
		        *Command: CmdCopies
		        {
		            *Order: DOC_SETUP.13
		            *Cmd: "<1B>&l1X"
		        }
			}
		}
    }
}

