*Feature: SorterMode
{
    *FeatureType: DOC_PROPERTY
    *Name: "Sorter Mode"
    *DefaultOption: Mailbox
    *Option: Mailbox
    {
        *Name: "Mailbox"
    }
    *Option: JobSeparation
    {
        *Name: "Job Separation"
    }
    *Option: Stacking
    {
        *Name: "Stacking"
    }
    *Option: Sorting
    {
        *Name: "Sorting"
    }
}
*BlockMacro: bmSetSorterMode
{
	*switch: SorterMode
	{
		*case: Mailbox
	    {
			*Command: CmdSelect
			{
				*Order: JOB_SETUP.3
				*Cmd: "@PJL SET SEVENBIN=MAILBOX<0A>"
			}
	    }
	    *case: JobSeparation
	    {
			*Command: CmdSelect
			{
				*Order: JOB_SETUP.3
				*Cmd: "@PJL SET SEVENBIN=JOBSEPARATION<0A>"
+					  "@PJL SET OUTBIN=SORTER<0A>"
			}
	    }
	    *case: Stacking
	    {
			*Command: CmdSelect
			{
				*Order: JOB_SETUP.3
				*Cmd: "@PJL SET SEVENBIN=STACKING<0A>"
+					  "@PJL SET OUTBIN=SORTER<0A>"
			}
	    }
	    *case: Sorting
	    {
			*Command: CmdSelect
			{
				*Order: JOB_SETUP.3
				*Cmd: "@PJL SET SEVENBIN=SORTING<0A>"
+					  "@PJL SET OUTBIN=SORTER<0A>"
			}
	    }
	}
}
*Feature: OutputOptions
{
	*FeatureType: PRINTER_PROPERTY
	*rcNameID: =OUTPUT_OPTIONS
    *HelpIndex: 2000
	*DefaultOption: NotInstalled
	*Option: NotInstalled
	{
		*rcNameID: =NOT_INSTALLED
		*DisabledFeatures: LIST(Stapling)
	}
	*Option: StapleStacker
	{
		*rcNameID: =STAPLE_STACKER
		*switch: Finishing
		{
			*case: DeviceStaple
			{
				*switch: StaplePosition
				{
					*case: UpperLeft
					{
						*switch: Orientation
						{
							*case: PORTRAIT
							{
								*Command: CmdSelect
								{
									*Order: JOB_SETUP.4
									*Cmd: "@PJL SET STAPLE=ONEPORT<0A>"
								}
							}
							*case: LANDSCAPE_CC90
							{
								*Command: CmdSelect
								{
									*Order: JOB_SETUP.4
									*Cmd: "@PJL SET STAPLE=ONELAND<0A>"
								}
							}
						}
					}
					*case: Left
					{
						*Command: CmdSelect
						{
							*Order: JOB_SETUP.4
							*Cmd: "@PJL SET STAPLE=TWO<0A>"
						}
					}
					*case: Top
					{
						*Command: CmdSelect
						{
							*Order: JOB_SETUP.4
							*Cmd: "@PJL SET STAPLE=TWO<0A>"
						}
					}
				}
			}
			*default
			{
			}
		}
	}
	*Option: SevenBinSorter
	{
		*rcNameID: =SORTER
		*DisabledFeatures: LIST(Stapling)
		*InsertBlock: =bmSetSorterMode
	}
}
